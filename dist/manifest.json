{
    "manifest_version": 2,
    "name": "Teemo Wallet NEO3",
    "description": "NEO browser extension",
    "version": "1.0.1",
    "browser_action": {
        "default_title": "Teemo Wallet NEO3",
        "default_icon": "icon48.png",
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        "js/inject.js"
    ],
    "background": {
        "scripts": [
            "lib/neo-thinsdk.js",
            "js/scoket.js",
            "js/account.js",
            "js/background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "notifications",
        "storage",
        "unlimitedStorage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/content.js"
            ]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}